<!-- signup-modal -->
  <div class="modal fade common-modal" id="signupmodal" tabindex="-1" role="dialog" >
    <div class="modal-dialog modal-dialog-centered max-WT-767" role="document">
      <div class="modal-content">
        <button type="button" class="close modal-cross" data-dismiss="modal" aria-label="Close">
          <img src="/assets/cross-icon.png" alt="Cross">
        </button>
        <div class="modal-body custome-modla-body min-ht-450 d-flex align-items-center">
          <div class="form-block p0 mb0">
            <h3 class="modal_form_heading">Register Now</h3>
            <!-- <form class="main-form max-WT-480 pb0"> -->
            <%= form_with scope: :user,url: users_path,:html => {class: "main-form max-WT-480 pb0",id: "signup_page"},local: true  do |f| %>
              <div class="form-group row">
                <div class="col-md-12">
                  <%= f.text_field :name, class: 'form-control',placeholder: "Name",id: "signup_name" %>
                  <!-- <input type="text" class="form-control" placeholder="Name"> -->
                </div>
              </div>
              <div class="form-group row">
                <div class="col-md-12">
                  <!-- <input type="email" class="form-control" placeholder="Email"> -->
                  <%= f.email_field :email, class: 'form-control',placeholder: "Email",id: "signup_email" %>
                </div>
              </div>
              <div class="form-group row number-block">
                <div class="col-md-12 d-flex">
                  <div class="country-code">
                  <%= f.collection_select(:mobile_code, ISO3166::Country.countries.sort_by(&:name).collect { |c| [ c.country_code, "#{c.name}  +#{c.country_code}" ] }, :first, :last, {include_blank: 'code'},class: "form-control select_style2") %>
                   <!--  <select class="form-control select_style2">
                     <option selected>+91</option>
                     <option >+123</option>
                     <option>+212</option>
                   </select> -->
                  </div>
                  <!-- <input type="text" class="form-control" placeholder="Mobile No."> -->
                  <%= f.telephone_field :mobile_no, class: 'form-control',placeholder: "Mobile No.",id: "signup_mobile" %>
                </div>
              </div>
             <div class="form-group row">
                <div class="col-md-12">
                   <%= f.collection_select(:country_name, ISO3166::Country.countries.sort_by(&:name).collect { |c| [ "#{c.name}" ] }, :first, :last, {include_blank: 'Select Country '},class: "form-control select_style2") %>
                 <!--  <select class="form-control select_style2">
                     <option selected>Country Name</option>
                     <option >India</option>
                     <option>Country Name</option>
                  </select> -->
                </div>
              </div>
              <div class="form-group row">
                <div class="col-md-12">
                  <!-- <input type="password" class="form-control" placeholder="Password"> -->
                  <%= f.password_field :password, class: 'form-control',placeholder: "Password",id: "signup_password" %>
                </div>
              </div>
              <div class="form-group row">
                <div class="col-md-12">
                  <!-- <input type="password" class="form-control" placeholder="Confirm Password"> -->
                  <%= f.password_field :password, class: 'form-control',placeholder: "Confirm Password",id: "signup_confirm_password",name: "user[confirm_password]" %>
                </div>
              </div>
              <div class="form-group row">
                <div class="col-lg-12">
                  <label class="checkbox-design">
                    <input type="checkbox" name="">
                    <span></span>I have read the <a class="terms-anchor" href="terms.html">Terms and Conditions</a>
                  </label>
                </div>
              </div>
              <div class="form-group row">
                <div class="col-md-12">
                  <button type="submit" class="btn btn-block btn-login btn-orrange">Sign Up</button>
                </div>
              </div>
              
              <div class="form-group row mb0">
                <div class="col-md-12">
                  <div class="dont-account">
                    <p>Already have an account? <a href="#loginModal" class="link-signup" data-toggle="modal" data-dismiss="modal">Sign In</a></p>
                  </div>
                </div>
              </div>
              <% end %>
            <!-- </form> -->
          </div>
        </div>
      </div>
    </div>
  </div>


