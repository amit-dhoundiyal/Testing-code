    },
    "user[mobile_no]":{
      required: true,
      minlength: 8,
      maxlength: 15
    },
    "user[mobile_code]":{
      required: true
     },
    "user[country_name]":{
      required: true
    }

     
  },
 
  messages: {
    "user[first_name]":{
      required: "This field is required",
      minlength: "Name should not be more less 3 characters",
      maxlength: "Name should not be more than 30 characters"
     
    },
    "user[email]":{
      required: "This field is required",
      minlength: "Password should not be less than 6 characters",
      maxlength: "Email should not be more than 30 characters"

    },
    "user[password]":{
      required: "This field is required",
      minlength: "Password should not be less than 6 characters",
      maxlength: "Password should not be more than 20 characters"
     

    },
    "user[confirm_password]":{
      required: "This field is required",
      minlength: "Password should not be less than 6 characters",
      maxlength: "Password should not be more than 20 characters"
     

    },
    "user[mobile_no]":{
      required: "This field is required",
      minlength: "Mobile Number should not be less than 8 digits",
      maxlength: "Mobile Number should not be more than 15 digits"
    },
    "user[mobile_code]":{
      required: "This field is required"
     },
    "user[country_name]":{
      required: "This field is required"
    }
  }
 });
});
</script>

<script type="text/javascript">

$(document).click(function(){
       $("#forgot_page").validate({
     
      errorPlacement: function (error, element) {
   error.insertAfter(element);
   },
    rules: {
    "user[forgot_email]":{
      required: true,
      maxlength: 30,
      email: true
     
      }
    },
    messages: {
      "user[forgot_email]":{
      required: "This field is required",
      minlength: "Password should not be less than 6 characters",
      maxlength: "Email should not be more than 30 characters"

    }
  }
 });
});
</script>

<script type="text/javascript">
  
$(document).click(function(){
       $("#address_page").validate({
     
      errorPlacement: function (error, element) {
   error.insertAfter(element);
   },
    rules: {
    "user[address]":{
      required: true,
      minlength: 3
     
      }
    },
    messages: {
      "user[address]":{
      required: "This field is required",
      minlength: "Address should not be less than 3 characters"

    }
  }
 });
});

</script>
  